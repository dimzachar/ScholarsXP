# App
NODE_ENV=development
RATE_LIMIT_ENABLED=false
DEV_CSP_ENABLED=false

# Next
NEXT_PUBLIC_APP_URL=http://localhost:3002

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# OpenRouter / LLMs (optional)
OPENROUTER_API_KEY=your_openrouter_key

# Reddit (optional)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USERNAME=your_reddit_username
REDDIT_PASSWORD=your_reddit_password

# Privy Authentication
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id
PRIVY_APP_SECRET=your_privy_app_secret

# Shinami Gas Station (Movement Network)
# Only needs Gas Station rights (not Wallet Services)
SHINAMI_GAS_STATION_KEY=your_shinami_gas_station_key

# Movement Network Vote Contract (Option 2)
# Set after deploying contracts/sources/vote.move
NEXT_PUBLIC_VOTE_CONTRACT=
NEXT_PUBLIC_MOVEMENT_RPC_URL=https://testnet.movementnetwork.xyz/v1

# Other
SUPPRESS_LOGS=false
